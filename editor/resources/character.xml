<panel layout="GridBagLayout">
	<vbox>
		<gridbagconstraints
			gridx="0"
			gridy="0"
			weightx="1"
			weighty="1"
			gridheight="1"
			anchor="GridBagConstraints.NORTHWEST"
			fill="GridBagConstraints.HORIZONTAL"
			/>
			<!--
			-->
		<hbox>				
			<label text="Name:" />
			<textfield size="20" id="name" />
		</hbox>
		<hbox>
			<label text="Health:" />
			<textfield size="4" id="health" />
		</hbox>
		<hbox>
			<label text="X: " />
			<textfield size="3" id="x" />
		</hbox>
		<hbox>
			<label text="Y: " />
			<textfield size="3" id="y" />
		</hbox>
		<hbox>
			<label text="Block: " />
			<spinner id="block" />
		</hbox>
		<hbox>
			<label text="Path: " />
			<list id="paths" />
		</hbox>
		<hbox>
			<label text="Map" />
			<spinner id="map" />
		</hbox>
	</vbox>
</panel>
